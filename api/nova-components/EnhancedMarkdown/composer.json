{
    "name": "alfonsobries/enhanced-markdown",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Alfonsobries\\EnhancedMarkdown\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alfonsobries\\EnhancedMarkdown\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
